{
    "name": "symfony-flex-demo/easyadmin",
    "type": "project",
    "license": "MIT",
    "description": "A minimal EasyAdmin FOSCKEditor Flex project",
    "require": {
    },
    "require-dev": {
    },
    "bin": ["bin/console"],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "initialize": [
            "composer exec -- console doctrine:database:create",
            "composer exec -- console doctrine:migrations:diff --quiet",
            "composer exec -- console doctrine:migrations:migrate --no-interaction --quiet",
            "composer exec -- console ckeditor:install --quiet",
            "composer exec -- console assets:install --symlink --relative"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
