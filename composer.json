{
    "name": "symfony-flex-demo/easyadmin",
    "type": "project",
    "license": "MIT",
    "description": "A minimal EasyAdmin FOSCKEditor Flex project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "bin": ["bin/console"],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": [
        ],
        "console": [
            "console"
        ],
        "initialize": [
            "doctrine:database:create",
            "doctrine:migrations:diff --quiet",
            "doctrine:migrations:migrate --no-interaction --quiet",
            "ckeditor:install --quiet",
            "assets:install --symlink --relative"
        ],
        "security:check": [
            "security-checker security:check"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "require": {
        "easycorp/easyadmin-bundle": "^1.17",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4 || ^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^3.4 || ^4.0"
    }
}
